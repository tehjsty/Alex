<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import com.jfoenix.controls.JFXToolbar?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.ToolBar?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="iim.controller.ConfigController"
            prefHeight="400.0" prefWidth="600.0" >
    <children>
        <StackPane layoutX="184.0" layoutY="65.0" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0"
                   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"  >
            <children>
                <JFXToolbar>
                    <left>
                        <VBox prefHeight="358.0" prefWidth="98.0">
                            <children>
                                <Label prefHeight="403.0" prefWidth="106.0" text="Label"/>
                            </children>
                        </VBox>
                    </left>
                    <center>
                        <HBox alignment="CENTER" prefHeight="0.0" prefWidth="127.0">
                            <children>
                                <Button fx:id="webButtonHelp" mnemonicParsing="false" text="Help"
                                        onAction="#buttonAction"/>

                                <JFXButton fx:id="scanButton" onMouseClicked="#handleButtonAction" prefHeight="35.0"
                                           prefWidth="137.0" text="Network Scan"/>
                            </children>
                        </HBox>
                    </center>
                </JFXToolbar>
            </children>
        </StackPane>
        <ToolBar layoutX="14.0" layoutY="-134.0" prefHeight="40.0" prefWidth="200.0">
            <items>
                <Button mnemonicParsing="false" text="Button"/>
            </items>
        </ToolBar>
    </children>

</AnchorPane>
